                "format":"any",
                "query":["none"],
                "params":["none"],
                "body":[
                    "username : string",
                    "password : string",
                    "email : string",
                    "lastname : string",
                    "firstname : string",
                    "photo : string nullable"
                ],
                "headers":["none"],
                "response":[
                    "username : string",
                    "email : string",
                    "lastname : string",
                    "firstname : string",
                    "photo : string nullable"
                ]
            },
